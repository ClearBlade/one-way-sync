{
    "name": "@clearblade/one-way-sync",
    "version": "1.1.3",
    "description": "",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watchAll",
        "prepublish": "tsc",
        "prebuild": "del dist/",
        "build": "tsc",
        "dist": "cp package.json dist && cp Readme.md dist && cd dist && npm publish",
        "postbuild": "cpy 'static/promise-polyfill/**' ../dist/ --cwd=src/ --parents",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "pre-commit": [
        "lint",
        "test"
    ],
    "lint-staged": {
        "*.{ts}": "npm run lint"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-typescript": "^7.8.3",
        "@clearblade/code-service-types": "^1.1.0",
        "@types/jest": "^25.1.2",
        "@typescript-eslint/eslint-plugin": "^2.19.2",
        "@typescript-eslint/parser": "^2.19.2",
        "babel-jest": "^25.1.0",
        "cpy-cli": "^3.0.0",
        "del-cli": "^3.0.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^25.1.0",
        "jest-cli": "^25.1.0",
        "lint-staged": "^9.5.0",
        "npm-run-all": "^4.1.5",
        "pre-commit": "^1.2.2",
        "prettier": "^1.19.1",
        "typescript": "^3.7.5"
    },
    "jest": {
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "js",
            "jsx",
            "ts",
            "tsx"
        ],
        "setupFiles": [
            "./utils/setupTests.ts"
        ]
    },
    "directories": {
        "example": "examples",
        "lib": "lib"
    },
    "dependencies": {
        "@clearblade/messaging-utils": "^1.0.1",
        "core-js": "^3.6.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ClearBlade/one-way-sync.git"
    },
    "bugs": {
        "url": "https://github.com/ClearBlade/one-way-sync/issues"
    },
    "homepage": "https://github.com/ClearBlade/one-way-sync#readme"
}
